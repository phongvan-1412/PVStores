@model IEnumerable<string>
@Html.Partial("~/Views/Shared/_HeaderInnerPagePartial.cshtml")

<main id="main">
    <section class="breadcrumbs">
        <div class="container">

            <div class="d-flex justify-content-between align-items-center">
                <h2>Log In</h2>
                <ol>
                    <li><a href="/Home/Index">Home</a></li>
                    <li><a href="/Account/Index"></a>Category</li>
                </ol>
            </div>

        </div>
    </section>

    <section class="inner-page">
        <div class="container">
            <h1>This is login</h1>
            @foreach (var item in Model)
            {
                <a asp-area="" asp-controller="Account" asp-action="Signin" asp-route-provider="@item" class="btn btn-primary">@item</a>

            }
        </div>
    </section>
</main>

